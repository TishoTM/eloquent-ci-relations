{
    "name": "tishotm/eloquent-ci-relations",
    "description": "Eloquent case-insensitive relations",
    "require": {
    "illuminate/database": "^5.5.33 || ~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "TishoTM\\": "src/TishoTM/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/models.php"
        ],
        "psr-4": {
            "TishoTM\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tihomir Mihaylov",
            "email": "ttm@artwishstudio.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
